    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.4",
    "autoprefixer": "^10.2.0",
    "cross-env": "^7.0.3",
    "liquidjs": "^9.25.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.2",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.1.0",
    "tailwindcss": "^2.2.7",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1"
  }
